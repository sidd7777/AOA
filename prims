int minKey(int weight[][], bool mst[][],int a) 
{ 
    int min = 0, i,j,min_index; 
    int a=b;
    for (i = 0; i < b; i++) 
        if (mst[i][j] == false &&  weight[i][j] < min) 
            min = weight[i][j], min_index = ; 
  
    return min_index; 
} 
void main()
{
	int i,n,weight[v][v],vertex[v][v],
